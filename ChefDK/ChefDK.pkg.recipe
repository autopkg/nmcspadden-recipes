<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads latest ChefDK disk image and builds a package.</string>
    <key>Identifier</key>
    <string>com.bk.autopkg.pkg.chefdk</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>ChefDK</string>
    </dict>
    <key>ParentRecipe</key>
    <string>com.facebook.autopkg.download.chefdk</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>Copier</string>
            <key>Arguments</key>
            <dict>
                <key>source_path</key>
                <string>%pathname%/%app_name%</string>
                <key>destination_path</key>
                <string>%pkgroot%/Applications/%app_name%</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
